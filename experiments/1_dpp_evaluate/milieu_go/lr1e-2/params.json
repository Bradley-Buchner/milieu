{
    "process": "dpp_evaluate",
    "process_params": 
        {
            "n_processes": 1,
            "ppi_network": "data/networks/species_9606/bio-pathways/network.txt",
            "diseases_path": "data/associations/disgenet-associations.csv",
            "disease_subset": [],

            "n_folds": 10,

            "method_class": "LCI",
            "method_params": {
                "model_class": "LCIEmbModule",
