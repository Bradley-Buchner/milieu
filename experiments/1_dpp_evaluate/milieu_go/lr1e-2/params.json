                    "emb_hidden_size": 512, 
                  
                    "embeddings_path": "data/embeddings/pca_go_d1024.txt"
                },
                "cuda": true,
                "cuda_gpu": 3,
                "load": false, 
                "learning_rate": 1e-2,
                "weight_decay": 1e-5,
                "num_epochs":  10,
                "batch_size": 200,
                "save_summary_steps": 40,
                "num_workers": 4,
                "primary_metric": "recall_at_100"
            }
        }
