{
    "process": "disease_subgraph",
    "process_params": {
        "n_processes": 1,
        "ppi_network": "data/networks/bio-pathways-network.txt",
        "diseases_path": "data/associations/disgenet-associations.csv",
        "disease_subset": [
            "C0235430", "C0003803", "C0001627", "C0008325"
        ],
        "method_to_preds": {
            "lci": "experiments/dpp_predict/lci"
        },
        "field_to_protein_data": {
            "protein_name": {
                "path": "data/protein/symbol_to_entrez.txt",
                "args": {
                    "delimiter": "\t",
                    "reverse": "True"
                }
            }, 
            "lci_weight": {
                "path": "experiments/dpp_predict/lci_new/norm_weights.csv",
                "args": {
                    "delimiter": ","
                }
            }
        },
        "num_preds": 5
    }
}