{
    "process": "dpp_evaluate",
    "process_params": 
        {
            "n_processes": 1,
            "ppi_network": "data/networks/bio-pathways-network.txt",
            "diseases_path": "data/associations/disgenet-associations.csv",
            "disease_subset": ["C0008325"],

            "n_folds": 10,

            "method_class": "GCN",
            "method_params": {
                "model_args": {
                    "cuda": true,
                    "devices": [0],

                    "optim_class": "Adam",
                    "optim_args": {
                        "lr": 1e-1,
                        "weight_decay": 0
                    },

                    "scheduler_class": "StepLR",
                    "scheduler_args": {
                        "step_size": 20,
                        "gamma": 0.5
                    },

                    "gcn_layer_configs": [
                        {
                            "class": "GraphConvolutionLayer",
                            "args": {
                                "out_features": 1,
                                "self_dropout": 1.0,
                                "bias": false
                            }
                        }
                    ]
                },

                "dataset_args": {
                    "num_examples": 30
                },

                "train_args": {
                    "num_epochs": 6,
                    "summary_period": 1,
                    "metric_configs": [
                        {"fn": "recall_at_100"}
                    ]
                },

                "valid_args": {
                    "metric_configs": [
                        {"fn": "recall_at_100"}
                    ]
                }
            }
        }
}