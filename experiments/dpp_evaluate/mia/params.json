{
    "process": "dpp_evaluate",
    "process_params": 
        {
            "n_processes": 1,
            "ppi_network": "data/networks/bio-pathways-network.txt",
            "diseases_path": "data/associations/disgenet-associations.csv",
            "disease_subset": ["C0001627", "C0008325"],

            "n_folds": 10,

            "method_class": "MIA",
            "method_params": {
                "model_args": {
                    "cuda": true,
                    "devices": [0],

                    "optim_class": "Adam",
                    "optim_args": {
                        "lr": 1e-2,
                        "weight_decay": 0
                    },

                    "scheduler_class": "StepLR",
                    "scheduler_args": {
                        "step_size": 20,
                        "gamma": 0.5
                    },

                    "gcn_layer_configs": [
                        {
                            "class": "GraphConvolution",
                            "args": {
                                "in_features": -1,
                                "out_features": 128
                            }
                        },
                        {
                            "class": "GraphConvolution",
                            "args": {
                                "in_features": 128,
                                "out_features": 512
                            }
                        }
                    ],
    
                    "fcn_layer_configs": [
                        {
                            "class": "Linear",
                            "args": {
                                "in_features": 512,
                                "out_features": 512
                            }
                        },
                        {
                            "class": "Linear",
                            "args": {
                                "in_features": 512,
                                "out_features": 1
                            }
                        }
                    ]
                },

                "batch_size": 20,

                "train_dataset_args": {
                    "num_examples": 60,
                    "frac_hidden": 0.25
                },

                "valid_dataset_args": {},

                "train_args": {
                    "num_epochs": 6,
                    "summary_period": 1,
                    "metric_configs": [
                        {"fn": "recall_at_100"}
                    ]
                },

                "valid_args": {
                    "metric_configs": [
                        {"fn": "recall_at_100"}
                    ]
                }
            }
        }
}